        <childPackages xsi:type="org.eclipse.epf.uma:ContentPackage" xmi:id="_hQwmYPZhEe2IOaTIFHQzew" name="desarrollar_modelo_basado_en_los_requisitos" guid="_hQwmYPZhEe2IOaTIFHQzew" presentationName="desarrollar modelo basado en los requisitos">
